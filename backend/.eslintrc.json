{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
      "ecmaVersion": "latest"
  },
  "rules": {
      "indent": ["error", 2],
      "max-len": ["error", { "code": 120 }],
      "class-methods-use-this": [0],
      "semi": ["error", "always"]
    }
}
